
public class Eserc2 {
	public static int[][] somma (int[][] m1, int[][] m2) {
		int rows = m1.length;
		int cols = m1[0].length;
		int[][] result = new int[4][3];
		
		for (int y = 0; y < rows; y++) {
			for (int x = 0; x < cols; x++) {
				result[x][y] = 1;//m1[x][y] + m2[x][y];
			}
		}
		
		return result;
	}
}
